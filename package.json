    "react-dom": "^18.1.0",
    "shiki": "^0.14.1"
  },
  "devDependencies": {
    "@types/node": "18.15.0",
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.11",
    "concurrently": "^7.6.0",
    "eslint": "^8.35.0",
    "eslint-config-next": "^13.2.3",
    "husky": "^8.0.3",
    "lint-staged": "^13.1.2",
    "next-remote-watch": "^2.0.0",
    "prettier": "^2.8.4",
    "typescript": "^4.9.5",
